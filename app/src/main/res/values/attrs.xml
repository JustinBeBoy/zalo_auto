<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CommonEditText">
        <attr name="android:inputType" />
        <attr name="android:lines" />
        <attr name="android:maxLines" />
        <attr name="android:enabled" />
        <attr name="android:text" />
        <attr name="android:hint" />
        <attr name="android:background" />
        <attr name="android:drawableStart" />
        <attr name="android:drawableEnd" />
    </declare-styleable>

    <declare-styleable name="CommonButton">
        <attr name="type" format="enum">
            <enum name="filled" value="0" />
            <enum name="outlined" value="1" />
        </attr>
        <attr name="android:backgroundTint" />
        <attr name="android:textColor" />
        <attr name="android:text" />
        <attr name="android:enabled" />
    </declare-styleable>

    <declare-styleable name="SwitchSettingView">
        <attr name="android:text" />
        <attr name="android:checked" />
        <attr name="isVisibleDivider" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CommonSwitch">
        <attr name="android:layout_width" />
    </declare-styleable>

</resources>